@model IEnumerable<FIVERR_PROJECT.Models.testimonial>

@{
    ViewBag.Title = "approvedtesti";
    Layout = "~/Views/Shared/_dashboad.cshtml";
}

<h1 style="text-align:center">APPROVED TESTIMONIAL</h1>
<hr />
<br />
<br /><br />

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Testimonial_Category)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Testimony)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.likes)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.comm)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Testimonial_Category)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Testimony)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.likes)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.comm)
        </td>

    </tr>
}

</table>
